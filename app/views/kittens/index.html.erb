<h1>Kittens</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Cuteness</th>
      <th>softness</th>
    </tr>
  </thead>
    <tbody>
        <% @kittens.each do |kit| %>
            <tr>
                <td><%= kit.name %></td>
                <td><%= kit.age %></td>
                <td><%= kit.cuteness %></td>
                <td><%= kit.softness %></td>
            </tr>
        <% end %>
    </tbody>
</table>